    <PackageTags>commandline;parsing</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\..\shared\Microsoft.Extensions.CommandLineUtils.Sources\**\*.cs" />
  </ItemGroup>

</Project>
